{

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "FeatureManagement": {
    "FeatureSwitch": true,
    "NewFeatureFlag": true,
    "GetItemById": true,
    "MessagesGreetingsFlag": {
      "EnabledFor": [
        {
          "Name": "MessagesGreetings",
          "Parameters": {
            "Set": [
              "5:20 pm",
              "Message"
            ]
          }
        }
      ]
    },

    "SwitchFlag": {
      "EnabledFor": [
        {
          "Name": "Switch",
          "Parameters": {
            "Set": [
              "On"
            ]
          }
        }
      ]
    },

    "MessageScheduleFlag": {
      "EnabledFor": [
        {
          "Name": "TimeWindow",
          "Parameters": {
            "Start": "10 Feb 2021 08:39:00 GMT",
            "End": "10 Feb 2021 08:40:00 GMT"
          }
        }
      ]
    },
    "BrowserFlag": {
      "EnabledFor": [
        {
          "Name": "BrowserFilter",
          "Parameters": {
            "AllowedBrowsers": [
              "Edg"
            ]
          }
        }
      ]
    },
    "DevEnvironment": {
      "EnabledFor": [
        {
          "Name": "DevEnv"
        },
        {
          "Name": "ProdTestAccounts",
          "Parameters": [
            "user123",
            "user456"
          ]
        }
      ]
    }

  }
}
