services:
  db:
    container_name: mysqldb
    environment:
      MYSQL_ROOT_PASSWORD: testroot
      MYSQL_USER: testroot
    image: mysql:5.6.50
    ports:
    - published: 3306
      target: 3306
    restart: always
    volumes:
    - d:\mysqldata:/var/lib/mysql:rw
  todoapp.api:
    build:
      context: D:\PPS SE\Training Projects\TodoApp.MVVM\src\TodoApp.API\TodoApp.API
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: TodoApp.API
      target: base
    container_name: TodoApp.API
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: todoappapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/TodoApp.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8080
      target: 80
    - target: 80
    - target: 443
    tty: true
    volumes:
    - D:\PPS SE\Training Projects\TodoApp.MVVM\src\TodoApp.API\TodoApp.API:/app:rw
    - C:\Users\cjabuni\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\cjabuni\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\cjabuni\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\cjabuni\.nuget\packages:/root/.nuget/packages:ro
    - D:\PPS SE\Training Projects\TodoApp.MVVM\src\TodoApp.API\TodoApp.API:/src:rw
version: '3.4'